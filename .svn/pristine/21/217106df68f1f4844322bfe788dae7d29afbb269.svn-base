<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.apps.interestingapps.speeddial"
    android:versionCode="3"
    android:versionName="1.0.3" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="13" />

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:icon="@drawable/speed_dial_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".SpeedDialActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_speed_dial"
            android:windowSoftInputMode="stateVisible" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.google.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize" />

        <provider
            android:name=".SpeedDialContentProvider"
            android:authorities="@string/contentprovider_authority" >
        </provider>
    </application>

</manifest>